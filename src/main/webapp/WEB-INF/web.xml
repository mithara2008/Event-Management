<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
     <servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>Service_Provider.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
         
         
   
	<servlet>
		<servlet-name>ser_provInsert</servlet-name>
		<servlet-class>Service_Provider.ser_provInsert</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ser_provInsert</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UpdateServ</servlet-name>
		<servlet-class>Service_Provider.updateSerProviderServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>UpdateServ</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteServiceProvider</servlet-name>
		<servlet-class>Service_Provider.deleteServiceProviderServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>DeleteServiceProvider</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>taskInsert</servlet-name>
		<servlet-class>task_assignment.TaskInsertServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>taskInsert</servlet-name>
		<url-pattern>/insertT</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UpdateTaskAssign</servlet-name>
		<servlet-class>task_assignment.updateTaskServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>UpdateTaskAssign</servlet-name>
		<url-pattern>/updateTask</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteTaskAssign</servlet-name>
		<servlet-class>task_assignment.DeleteTaskServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>DeleteTaskAssign</servlet-name>
		<url-pattern>/deleteTask</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>Service_Provider.LogoutServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	
	
	
	
  <display-name>Signup</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    
  
    
  </welcome-file-list>
</web-app>